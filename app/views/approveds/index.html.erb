<p id="notice"><%= notice %></p>

<h1>Approveds</h1>

<table>
  <thead>
    <tr>
      <th>Venue name</th>
      <th>Venue address</th>
      <th>Venue ownername</th>
      <th>Approved at</th>
      <th>Approved by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @approveds.each do |approved| %>
      <tr>
        <td><%= approved.venue_name %></td>
        <td><%= approved.venue_address %></td>
        <td><%= approved.venue_ownername %></td>
        <td><%= approved.approved_at %></td>
        <td><%= approved.approved_by %></td>
        <td><%= link_to 'Show', approved %></td>
        <td><%= link_to 'Edit', edit_approved_path(approved) %></td>
        <td><%= link_to 'Destroy', approved, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Approved', new_approved_path %>
